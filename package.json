{
  "name": "expweb-bp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node ./dist/bin/www",
    "w:d": "NODE_ENV=development babel-watch ./server/bin/www",
    "w:s": "NODE_ENV=development webpack serve --config ./webpack.dev.config.js --mode development",
    "clean": "rm -rf dist",
    "b:s": "npm run clean && babel ./server --out-dir dist --copy-files",
    "b:c": "webpack --config ./webpack.config.js --mode production",
    "build": "npm run b:c && npm run b:s"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "hbs": "^4.1.1",
    "http-errors": "~1.6.3",
    "mongoose": "^5.12.0",
    "morgan": "~1.9.1",
    "webpack": "^5.24.0",
    "webpack-dev-middleware": "^4.1.0",
    "webpack-hot-middleware": "^2.25.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.17",
    "@babel/core": "^7.12.17",
    "@babel/node": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-minify": "^0.5.1",
    "babel-watch": "^7.4.0",
    "core-js": "^3.9.0",
    "css-loader": "^5.0.2",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-webpack-plugin": "^2.5.2",
    "mini-css-extract-plugin": "^1.3.8",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
